{
  "name": "@mendix/release-utils-internal",
  "version": "1.0.0",
  "description": "Set of helpers for release processes",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc --watch",
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "lint": "eslint --config ../../../.eslintrc.js --ext .jsx,.js,.ts,.tsx src/",
    "build-parser": "pegjs ./src/changelog-parser/parser/module/module.pegjs && pegjs ./src/changelog-parser/parser/widget/widget.pegjs",
    "prepare": "npm run build-parser && npm run build"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "pegjs": "^0.10.0",
    "rimraf": "^2.7.1",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist"
  ]
}
